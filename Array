#include<stdio.h>
#include<stdlib.h>
int main ()
{
     int a[10000] = {1, 4, 3, 2};     
    
    int n,i ; 
    
    scanf("%d\t",&n);
    for( i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }
        
       for (int i = n-1; i >= 0; i--) {     
        printf("%d ", a[i]);     
    }     
    return 0;    
}
